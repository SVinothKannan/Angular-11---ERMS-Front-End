{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "immedisassignment": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/immedisassignment",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": true,
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
             
              "src/assets/css/bootstrap.min.css",
              "src/assets/css/font-awesome.min.css",
              "src/assets/css/owl.carousel.css",
              "src/assets/css/owl.theme.css",
              "src/assets/css/owl.transitions.css",
              "src/assets/css/normalize.css",
              "src/assets/css/meanmenu.min.css",
              "src/assets/css/main.css",
              "src/assets/css/educate-custon-icon.css",
              "src/assets/css/morrisjs/morris.css",
              "src/assets/css/scrollbar/jquery.mCustomScrollbar.min.css",
              "src/assets/css/metisMenu/metisMenu.min.css",
              "src/assets/css/calendar/fullcalendar.min.css",
              "src/assets/css/calendar/fullcalendar.print.min.css",
              "src/assets/css/touchspin/jquery.bootstrap-touchspin.min.css",
              "src/assets/css/datapicker/datepicker3.css",
              "src/assets/css/form/themesaller-forms.css",
              "src/assets/css/colorpicker/colorpicker.css",
              "src/assets/css/select2/select2.min.css",
              "src/assets/css/chosen/bootstrap-chosen.css",
              "src/assets/css/ionRangeSlider/ion.rangeSlider.css",
              "src/assets/css/ionRangeSlider/ion.rangeSlider.skinFlat.css",
              "src/assets/style.css",
              "src/assets/css/responsive.css",
              "src/styles.css",
              "src/assets/css/modals.css",
              "src/assets/css/animate.css",
              "src/assets/css/form/all-type-forms.css",
			  "src/assets/css/Fotorama.css"
            ],
            "scripts": [        
                  
              "src/assets/js/vendor/modernizr-2.8.3.min.js",
              "src/assets/js/vendor/jquery-1.12.4.min.js",
              "src/assets/js/bootstrap.min.js",
              "src/assets/js/wow.min.js",
              "src/assets/js/jquery-price-slider.js",
              "src/assets/js/jquery.meanmenu.js",
              "src/assets/js/owl.carousel.min.js",
              "src/assets/js/jquery.sticky.js",
              "src/assets/js/jquery.scrollUp.min.js",
              "src/assets/js/scrollbar/jquery.mCustomScrollbar.concat.min.js",
              "src/assets/js/scrollbar/mCustomScrollbar-active.js",
              "src/assets/js/metisMenu/metisMenu.min.js",
              "src/assets/js/metisMenu/metisMenu-active.js",
              "src/assets/js/sparkline/jquery.sparkline.min.js",
              "src/assets/js/sparkline/jquery.charts-sparkline.js",
              "src/assets/js/touchspin/jquery.bootstrap-touchspin.min.js",
              "src/assets/js/touchspin/touchspin-active.js",
              "src/assets/js/colorpicker/jquery.spectrum.min.js",
              "src/assets/js/colorpicker/color-picker-active.js",
              "src/assets/js/datapicker/bootstrap-datepicker.js",
              "src/assets/js/datapicker/datepicker-active.js",
              "src/assets/js/tab.js",
              "src/assets/js/plugins.js",
              "src/assets/js/main.js",
              "src/assets/js/chosen/chosen.jquery.js","src/assets/js/chosen/chosen-active.js",
              "src/assets/js/select2/select2.full.min.js","src/assets/js/select2/select2-active.js",
              "src/assets/js/Customdrodown.js",
              "src/assets/js/Fotorama.js"			  
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "immedisassignment:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "immedisassignment:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "immedisassignment:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "immedisassignment:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "immedisassignment:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "immedisassignment",
  "schematics": {
    "@schematics/angular:component": {
      "styleext": "sass"
    }
  }
}